{
    "name": "node-playground",
    "version": "1.0.0",
    "type": "module",
    "description": "",
    "repository": {
        "url": "https://github.com/iacoware/node-playground"
    },
    "engines": {
        "node": ">=22.18"
    },
    "scripts": {
        "cli": "node ./src/index.ts",
        "build": "npm run clean && tsc",
        "clean": "rimraf .build .coverage",
        "typecheck": "tsc --noemit",
        "typecheck:w": "tsc --noemit --watch",
        "tc": "npm run typecheck",
        "tc:w": "npm run typecheck:w",
        "lint": "eslint src",
        "test": "vitest --run",
        "test:w": "vitest",
        "check": "npm run lint && npm run typecheck && npm run test",
        "freshstart": "rimraf .git && git init && git add -A && git commit -m 'Initial commit'"
    },
    "devDependencies": {
        "@types/node": "22.18.8",
        "eslint": "9.39.2",
        "eslint-config-prettier": "10.1.8",
        "globals": "17.3.0",
        "prettier": "3.8.1",
        "rimraf": "6.1.3",
        "typescript": "5.9.3",
        "typescript-eslint": "8.55.0",
        "vitest": "4.0.18"
    },
    "author": "",
    "license": "ISC",
    "packageManager": "pnpm@10.18.0"
}
